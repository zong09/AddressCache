@using System.Collections.Specialized;
<table class="table table-striped table-bordered table-hover" id="dtActivity">
    <thead>
        <tr>
            <th>IP Address</th>
            <th>Host Name</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @{ 
            OrderedDictionary Dic = (OrderedDictionary)Model;
            String[] keys = new String[Dic.Keys.Count];
            Dic.Keys.CopyTo(keys, 0);
        }
        @for (int i = 0; i < Dic.Keys.Count; i++)
        {
            <tr>
                <td>@keys[i]</td>
                <td>@Dic[i]</td>
                <td>
                    <a href="JAVASCRIPT:AddressRemove('@keys[i]','@Dic[i]')" >Remove</a>
                </td>
            </tr>
        }
    </tbody>
</table>